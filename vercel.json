{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/cop30/:path((?!.*\\..*$).*)",
      "destination": "/apps/event-cop30/dist/index.html"
    },
    {
      "source": "/cop30/:path*",
      "destination": "/apps/event-cop30/dist/:path*"
    },
    {
      "source": "/y20/:path((?!.*\\..*$).*)",
      "destination": "/apps/event-y20/dist/index.html"
    },
    {
      "source": "/y20/:path*",
      "destination": "/apps/event-y20/dist/:path*"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    }
  ]
}